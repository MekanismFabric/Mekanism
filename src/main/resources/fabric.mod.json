{
	"schemaVersion": 1,
	"id": "mekanism-fabric",
	"version": "${version}",
	"name": "mekanism-fabric",
	"description": "Mekanism port to Fabric - 1.20+",
	"authors": [
		"TinyPandas",
		"hero-ku"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mekanism-fabric/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.panda.mekanismfabric.Mekanism"
		],
		"client": [
			"net.panda.mekanismfabric.MekanismClient"
		],
		"fabric-datagen": [
			"net.panda.mekanismfabric.MekanismDataGenerator"
		]
	},
	"mixins": [
		"mekanism-fabric.mixins.json",
		{
			"config": "mekanism-fabric.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}